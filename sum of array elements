#include <stdio.h>
int main(void) {
  int *arr;
  int n,sum=0;
  printf("Enter the number :");
  scanf("%d",&n);
  arr=(int*)malloc(n*sizeof(int));
  for(int i=0;i<n;i++)
    {
      scanf("%d",arr+i);
    }
  for(int i=0;i<n;i++)
    {
      sum+=arr;
    }
  printf("The sum is %d",sum);
  
  return 0;
}
